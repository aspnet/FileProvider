{
  "dependencies": {
    "dotnet-test-xunit": "1.0.0-rc3-000000-01",
    "Microsoft.AspNetCore.Testing": "1.0.1",
    "Microsoft.Extensions.FileProviders.Embedded": "1.0.1",
    "xunit": "2.1.0"
  },
  "testRunner": "xunit",
  "buildOptions": {
    "embed": {
      "include": "File.txt;sub/**;Resources/**"
    }
  },
  "frameworks": {
    "netcoreapp1.0": {
      "imports": [
        "dnxcore50",
        "portable-net451+win8"
      ],
      "dependencies": {
        "Microsoft.NETCore.App": {
          "version": "1.0.4",
          "type": "platform"
        },
        "System.Reflection.Extensions": "4.0.1",
        "System.Diagnostics.Process": "4.1.0"
      }
    },
    "net451": {
      "frameworkAssemblies": {
        "System.Runtime": {
          "type": "build"
        },
        "System.Reflection": {
          "type": "build"
        }
      }
    }
  }
}